{
  "name": "nome-do-seu-projeto",
  "version": "1.0.0",
  "description": "Descrição do seu projeto",
  "scripts": {
    "build:windows": "mingw32-make",
    "clean:windows": "mingw32-make cleanwindows",
    "build:unix": "make",
    "clean:unix": "make cleanunix",
    "clean:os": "npm run clean:windows || npm run clean:unix",
    "build:os": "npm run build:windows || npm run build:unix",
    "build":"npm run build:os",
    "clean":"npm run clean:os",
    "clear": "npm run clear:os",
    "clear:os": "npm run clear:windows || npm run clear:unix",
    "clear:windows": "cls",
    "clear:unix": "clear",
    "open":"./main || main",
    "dev": "npm run build && npm run clean && npm run clear && npm run open"
  },
  "author": "Seu Nome",
  "license": "GNU",
  "dependencies": {
    "node": ">=10.0.0"
  },
  "extensionPack": [
    "ms-vscode.makefile-tools",
    "ms-vscode.cpptools"
  ]
}

